#!/usr/bin/env node

'use strict';

const server = require('../');

server.start({
  // apiURL: 'http://interview-api.waltzlabs.com/api/v1',
  apiURL: 'http://localhost:3001',
}, (err, instance) => {
  if (err) {
    console.error('Failed starting server');
    throw err;
  }

  console.log(`Listening on port ${instance.handle.address().port}`);
});
